{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "event_id": {
            "type": "string"
        },
        "source_type": {
            "type": "string",
            "enum": ["EXPORT_TASK"]
        },
        "timestamp": {
            "type": "integer"
        },
        "event_data": {
            "oneOf": [
                { "$ref": "./export_task_event_schema.json" }
            ]
        }
    },
    "required": ["event_id", "source_type", "timestamp", "event_data"]
}